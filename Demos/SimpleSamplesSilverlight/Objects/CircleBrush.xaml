<UserControl x:Class="FarseerGames.SimpleSamplesSilverlight.Objects.CircleBrush" xmlns="http://schemas.microsoft.com/client/2007" 
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                Width="640"
        Height="480"
>
	<Canvas IsHitTestVisible="False">
  <Ellipse x:Name="ellipse" Fill="White" Stroke="Black" StrokeThickness="2" Width="100" Height="100">
    <Ellipse.RenderTransform>
      <TransformGroup>
        <RotateTransform x:Name="rotateTransform" Angle="0" CenterX="0" CenterY="0"/>
        <TranslateTransform x:Name="translateTransform" X="0" Y="0"/>
      </TransformGroup>
    </Ellipse.RenderTransform>
  </Ellipse>
</Canvas>
</UserControl>